  position: absolute;
  top: 0;
  left: auto;
  right: 0;
  width: 100%;
  height: 100%;
  transform-origin: 0% 0%;
  display: block;
  box-sizing: border-box;
  transform: rotate(180deg);
  content: quot; quot; ;
  position: absolute;
  width: 3px;
  height: 100%;
  background: rgba(255, 255, 255, .6);
  animation: rightanimation 8s linear infinite;
