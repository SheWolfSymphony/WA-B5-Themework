  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  display: block;
  box-sizing: border-box;
  transform: rotate(0deg);
  content:  quot; quot; ;
  position: absolute;
  width: 3px;
  height: 100%;
  background: rgba(255, 255, 255, .6);
  animation: leftanimation 8s linear infinite;
  box-shadow: 0px 0px 10px rgba(77, 69, 69, .6);
}

