  position: absolute;
  top: auto;
  bottom: 0px;
  left: 0;
  display: block;
  box-sizing: border-box;
  transform: rotate(180deg);
  content: quot; quot; ;
  width: 100%;
  height: 3px;
  background: rgba(255, 255, 255, .6);
  animation: bottomanimation 8s linear infinite;
  box-shadow: 0px 0px 10px rgba(77, 69, 69, .6);
}

