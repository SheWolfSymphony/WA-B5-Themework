/* ----   Animations   ---- */

/*added*/
/* links animation */
@keyframes linksforfun {  /*--- this creates an animation that tells the website to change font color; it starts at 80% opacity and decreases to 60% ---*/
  from { color: rgba(0, 0, 0, .8), text-shadow: 0px 1px 4px rgba(77, 69, 69, .6); }
  to { color: rgba(255, 0, 0, .8); }
}
/* links animation */

/* desktop background animation */
@keyframes background-desktop {  /* this creates an animation that tells the backrgound to move; it will shift the background 600 pixels to the top in a single cycle */
	from { background-position: 0 0; }
	to { background-position: 0 -600px; }
}

@keyframes background-desktop-meta {
	from { background-position: 0 0; }
	to { background-position: 0 -600px; }
}
/* desktop background animation */

/* mobile background animation */
@keyframes background-mobile {
	from { background-position: 0 0; }
	to { background-position: 0 -300px; }
}

@keyframes background-mobile-meta {
	from { background-position: 0 0; }
	to { background-position: 0 300px; }
}
/* mobile background animation */

/*Fancy Border Animation*/
@keyframes topanimation {
  0% {
  transform:scaleX(0);
  transform-origin: left;
  }
  50%
  {
    transform:scaleX(1);
  transform-origin: left;
  }
  50.1%
  {
    transform:scaleX(1);
  transform-origin: right;
    
  }
  
  100%
  {
    transform:scaleX(0);
  transform-origin: right;
    
  }
} 

@keyframes leftanimation {
  0% {
  transform:scaleY(0);
  transform-origin: bottom;
  }
  50%
  {
    transform:scaleY(1);
  transform-origin: bottom;
  }
  50.1%
  {
    transform:scaleY(1);
  transform-origin: top;
    
  }
  
  100%
  {
    transform:scaleY(0);
  transform-origin: top;
    
  }
} 
/*Fancy border animation*/
/* ----   Animations   ---- */


